{
  "extends": [
    "airbnb-base",
    "plugin:eslint-comments/recommended",
    "plugin:json/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": [
    "import",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off"
  }
}
